{
  "name": "anfifrontend",
  "version": "0.1.0",
  "author": "John Doe <john@doe.com>",
  "private": true,
  "main": "electron/main.js",
  "homepage": "./",
  "dependencies": {
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^13.5.0",
    "axios": "^1.1.3",
    "canvas2image": "^1.0.5",
    "electron-is": "^3.0.0",
    "history": "^5.3.0",
    "html2canvas": "^1.4.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.4.3",
    "react-scripts": "5.0.1",
    "web-vitals": "^2.1.4"
  },
  "build": {
    "appId": "com.app.id",
    "productName": "React Electron App",
    "files": [
      "build/**/*",
      "electron/main.js",
      "package.json"
    ],
    "extends": null,
    "asar": true,
    "asarUnpack": [
      "**/dict/*"
    ],
    "mac": {
      "category": "public.app-category.developer-tools"
    },
    "linux": {
      "icon": "build",
      "target": [
        "AppImage",
        "deb"
      ]
    }
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "PUBLIC_URL=/ react-scripts build",
    "build-electron": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "electron:start": "concurrently \"yarn start\" \"wait-on http://localhost:3000 && electron .\"",
    "electron:build": "yarn build-electron && electron-builder --publish never"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@types/react-router-dom": "^5.3.3",
    "add": "^2.0.6",
    "concurrently": "^7.6.0",
    "electron": "^22.0.0",
    "electron-builder": "^23.6.0",
    "react-router-dom": "^6.6.0",
    "wait-on": "^7.0.1",
    "yarn": "^1.22.19"
  }
}
